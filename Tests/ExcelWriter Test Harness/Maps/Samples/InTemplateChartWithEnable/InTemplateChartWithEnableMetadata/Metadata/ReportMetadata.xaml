        <!-- =============================================================== -->
        <!-- InTemplateChart based template                                  -->
        <!-- =============================================================== 
        <Sheet SheetName="In Template Chart Sheet"
               PartId="InTemplateChartWithEnableDataPart"
               DesignerSheetName="" -->

        <Sheet SheetName="In Template Chart Sheet"
               PartId="InTemplateChartWithEnableDataPart">
            
            <ContentControl ContentKey="InTemplateChartWithEnableTemplate" />
            
        </Sheet>
        
    </ExcelDocumentMetadata.Sheets>

</ExcelDocumentMetadata>